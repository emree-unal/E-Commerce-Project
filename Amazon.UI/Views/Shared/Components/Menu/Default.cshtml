@model List<Category>

<div class="list-group">

    <a asp-action="Index" asp-controller="Home" class="list-group-item list-group-item-action">AnaSayfa</a>
    @foreach (var item in Model)
    {
        <a asp-action="Index" asp-controller="Home" asp-route-categoryId="@item.CategoryId" class="list-group-item list-group-item-action">@item.CategoryName</a>
    }


</div>
